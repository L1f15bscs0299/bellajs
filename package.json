{
  "version": "4.1.5",
  "name": "bellajs",
  "description": "A useful helper for any javascript program",
  "homepage": "http://techpush.net",
  "repository": {
    "type": "git",
    "url": "git@github.com:ndaidong/bella.js.git"
  },
  "author": "Dong Nguyen <ndaidong@gmail.com> (https://twitter.com/ndaidong)",
  "main": "./index.js",
  "engines": {
    "node": ">= 4.0"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha --reporter nyan --timeout 5000",
    "coverage": "./node_modules/.bin/nyc --require traceur mocha",
    "report": "npm run coverage && ./node_modules/.bin/nyc report --reporter=lcov",
    "coveralls": "npm run report && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "devDependencies": {
    "chai": "latest",
    "coveralls": "latest",
    "mocha": "latest",
    "nyc": "latest",
    "traceur": "latest"
  },
  "keywords": [
    "detection",
    "manipulation",
    "templating",
    "dom",
    "event",
    "datetime",
    "scheduler",
    "util"
  ],
  "license": "MIT",
  "maintainers": [
    "ndaidong <ndaidong@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/ndaidong/bella.js/issues"
  },
  "directories": {
    "test": "test"
  }
}
